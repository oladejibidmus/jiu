            "files": [
                {
                    "path": "src/components/Button.tsx",
                    "type": "registry:component"
                },
                {
                    "path": "src/components/ui/icons/ArrowAnimated.tsx",
                    "type": "registry:component"
                },
                {
                    "path": "src/components/ui/icons/TremorPlaceholder.tsx",
